[access]
	inheritFrom = All-Projects
[access "refs/*"]
	owner = group user/AJOY DAS (shikkanobish)
[access "refs/for/refs/*"]
	read = group user/AJOY DAS (shikkanobish)
	push = group user/AJOY DAS (shikkanobish)
	label-Code-Review = -2..+2 group user/AJOY DAS (shikkanobish)
	label-Verified = -1..+1 group user/AJOY DAS (shikkanobish)
	rebase = group user/AJOY DAS (shikkanobish)
	removeReviewer = group user/AJOY DAS (shikkanobish)
	submit = group user/AJOY DAS (shikkanobish)
[access "refs/heads/*"]
	read = group user/AJOY DAS (shikkanobish)
	create = group user/AJOY DAS (shikkanobish)
	push = +force group user/AJOY DAS (shikkanobish)
	pushMerge = group user/AJOY DAS (shikkanobish)
[access "refs/tags/*"]
	push = group user/AJOY DAS (shikkanobish)
	push = +force group user/AJOY DAS (shikkanobish)
